<p id="notice"><%= notice %></p>

<h1>Workflow Packages</h1>

<table>
  <thead>
    <tr>
      <th>Workflow</th>
      <th>Name</th>
      <th>Description</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @workflow_packages.each do |workflow_package| %>
      <tr>
        <td><%= workflow_package.workflow %></td>
        <td><%= workflow_package.name %></td>
        <td><%= workflow_package.description %></td>
        <td><%= workflow_package.user %></td>
        <td><%= link_to 'Show', workflow_package %></td>
        <td><%= link_to 'Edit', edit_workflow_package_path(workflow_package) %></td>
        <td><%= link_to 'Destroy', workflow_package, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Workflow Package', new_workflow_package_path %>
